{
  "name": "my-medusa-store",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "etl:opencart": "medusa exec ./src/scripts/opencart-etl.ts",
    "etl:server": "medusa exec ./src/etl/server.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.658.1",
    "@json2csv/plainjs": "^7.0.6",
    "@medusajs/admin-sdk": "2.11.3",
    "@medusajs/auth": "^2.11.3",
    "@medusajs/auth-emailpass": "^2.11.3",
    "@medusajs/framework": "2.11.3",
    "@medusajs/medusa": "2.11.3",
    "aws-sdk": "2.1692.0",
    "axios": "^1.13.2",
    "axios-cookiejar-support": "^6.0.4",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "he": "^1.2.0",
    "mysql2": "^3.11.3",
    "p-limit": "^4.0.0",
    "sanitize-html": "^2.17.0",
    "tough-cookie": "^5.1.2"
  },
  "devDependencies": {
    "@medusajs/cli": "^2.11.3",
    "@medusajs/test-utils": "2.11.3",
    "@swc/core": "^1.15.0",
    "@swc/jest": "^0.2.36",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "patch-package": "^8.0.1",
    "pg": "^8.13.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
