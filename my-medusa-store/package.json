{
  "name": "my-medusa-store",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "migrate": "TS_NODE_TRANSPILE_ONLY=true npx medusa db:migrate",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.671.0",
    "@aws-sdk/s3-request-presigner": "^3.671.0",
    "@medusajs/admin-sdk": "2.11.3",
    "@medusajs/auth": "^2.11.3",
    "@medusajs/auth-emailpass": "^2.11.3",
    "@medusajs/cli": "2.11.3",
    "@medusajs/file": "^2.11.3",
    "@medusajs/file-s3": "^2.11.3",
    "@medusajs/framework": "2.11.3",
    "@medusajs/medusa": "2.11.3",
    "@techlabi/medusa-marketplace-plugin": "^0.46.0",
    "bcryptjs": "^3.0.3",
    "busboy": "^1.6.0",
    "lucide-react": "^0.554.0",
    "mime": "^4.1.0",
    "pg": "^8.16.3",
    "razorpay": "^2.9.6",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.11.3",
    "@swc/core": "^1.15.0",
    "@swc/jest": "^0.2.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/busboy": "^1.5.4",
    "@types/he": "^1.2.3",
    "@types/jest": "^29.5.13",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "mysql2": "^3.15.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
