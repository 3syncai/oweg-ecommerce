{
    "rewrites": [
        {
            "source": "/backend-api/:path*",
            "destination": "https://api.oweg.itshover.com/:path*"
        },
        {
            "source": "/app/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/app/(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/app",
            "destination": "/index.html"
        },
        {
            "source": "/",
            "destination": "/index.html"
        }
    ]
}